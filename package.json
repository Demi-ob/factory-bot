{
  "name": "factory-bot",
  "description": "A factory library for Node.js and JavaScript inspired by factory_girl",
  "main": "./main.js",
  "module": "./src/index",
  "version": "6.1.4",
  "keywords": [
    "factory",
    "factory-bot",
    "factory-girl",
    "test",
    "bdd",
    "tdd",
    "fixture",
    "mongodb"
  ],
  "repository": "https://github.com/ratson/factory-bot.git",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ratson/factory-bot/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ratson/factory-bot/raw/master/LICENSE"
    }
  ],
  "dependencies": {
    "chance": "^1.0.18",
    "esm": "3.2.25"
  },
  "devDependencies": {
    "bluebird": "^3.5.3",
    "bookshelf": "^0.14.2",
    "chai": "^4.2.0",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.12.1",
    "eslint-config-concise": "^0.36.0",
    "eslint-config-concise-esnext": "^0.36.0",
    "eslint-config-concise-import": "^0.36.0",
    "eslint-config-concise-mocha": "^0.36.0",
    "husky": "^1.3.1",
    "istanbul": "^1.0.0-alpha.2",
    "knex": "^0.16.3",
    "lint-staged": "^8.1.0",
    "mocha": "^8.1.3",
    "mocha-eslint": "^5.0.0",
    "mongodb": "^3.1.12",
    "mongoose": "^5.4.6",
    "prettier-eslint-cli": "^5.0.0-beta.0",
    "redux-orm": "^0.12.2",
    "sequelize": "^4.42.0",
    "should": "^13.2.3",
    "sinon": "^7.2.3",
    "sinon-chai": "^3.3.0",
    "sqlite3": "^5.0.0"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha",
    "prepublishOnly": "npm test"
  },
  "eslintConfig": {
    "extends": [
      "concise",
      "concise-esnext",
      "concise-import"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "no-multi-assign": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-unused-expressions": "off"
    }
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  },
  "mocha": {
    "recursive": true,
    "require": "esm",
    "exit": true
  }
}
