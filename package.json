{
  "name": "factory-bot",
  "description": "A factory library for Node.js and JavaScript inspired by factory_girl",
  "main": "./main.js",
  "module": "./src/index",
  "version": "5.2.0",
  "keywords": [
    "factory",
    "test",
    "bdd",
    "tdd",
    "fixture"
  ],
  "repository": "https://github.com/ratson/factory-bot.git",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/ratson/factory-bot/issues"
  },
  "licenses": [
    {
      "type": "MIT",
      "url": "https://github.com/ratson/factory-bot/raw/master/LICENSE"
    }
  ],
  "dependencies": {
    "chance": "^1.0.16",
    "esm": "^3.0.72"
  },
  "devDependencies": {
    "bluebird": "^3.5.1",
    "bookshelf": "^0.13.3",
    "chai": "^4.1.2",
    "chai-as-promised": "^7.1.1",
    "eslint": "^5.2.0",
    "eslint-config-concise": "^0.29.2",
    "eslint-config-concise-esnext": "^0.28.2",
    "eslint-config-concise-import": "^0.28.0",
    "eslint-config-concise-mocha": "^0.28.2",
    "husky": "^0.14.3",
    "istanbul": "^1.0.0-alpha.2",
    "knex": "^0.15.2",
    "lint-staged": "^7.2.0",
    "mocha": "^5.2.0",
    "mocha-eslint": "^4.1.0",
    "mongodb": "^3.1.1",
    "mongoose": "^5.2.5",
    "prettier-eslint-cli": "^5.0.0-beta.0",
    "redux-orm": "^0.12.1",
    "sequelize": "^3.24.0",
    "should": "^13.2.2",
    "sinon": "^6.1.4",
    "sinon-chai": "^3.2.0",
    "sqlite3": "^4.0.2"
  },
  "scripts": {
    "lint": "eslint src test",
    "test": "NODE_ENV=test mocha",
    "prepublish": "npm run test",
    "precommit": "lint-staged"
  },
  "eslintConfig": {
    "extends": [
      "concise",
      "concise-esnext",
      "concise-import"
    ],
    "rules": {
      "class-methods-use-this": "off",
      "no-multi-assign": "off",
      "no-param-reassign": "off",
      "no-plusplus": "off",
      "no-unused-expressions": "off"
    }
  },
  "lint-staged": {
    "*.js": [
      "prettier-eslint --write",
      "eslint --fix",
      "git add"
    ]
  }
}
